{
  "name": "styled-custom-elements",
  "version": "0.1.9",
  "description": "Styled API for Custom Elements",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.modern.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    "import": "./dist/index.modern.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "browserslist": [
    "last 2 years",
    "node >= 12"
  ],
  "scripts": {
    "clean": "rimraf coverage/ dist/ storybook-static/",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepack": "npm run clean && npm run build",
    "build": "microbundle",
    "dev": "microbundle watch",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:serve": "serve storybook-static",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keplersj/styled-custom-elements.git"
  },
  "keywords": [
    "css-in-js",
    "css",
    "styled",
    "webcomponents",
    "custom-elements"
  ],
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "funding": {
    "type": "github-sponsors",
    "url": "https://github.com/sponsors/keplersj"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keplersj/styled-custom-elements/issues"
  },
  "homepage": "https://github.com/keplersj/styled-custom-elements#readme",
  "devDependencies": {
    "@babel/core": "7.21.0",
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/html": "6.5.16",
    "@types/jest": "27.5.2",
    "@ungap/custom-elements": "1.1.1",
    "babel-loader": "9.1.2",
    "eslint": "8.35.0",
    "eslint-config-starstuff": "1.5.15",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "microbundle": "0.15.1",
    "prettier": "2.8.1",
    "rimraf": "4.1.3",
    "serve": "14.2.0",
    "storybook-builder-vite": "0.1.23",
    "ts-jest": "27.1.5",
    "typescript": "4.9.5",
    "vite": "3.0.9"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    }
  },
  "eslintConfig": {
    "extends": "starstuff/auto",
    "env": {
      "browser": true
    }
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/serialize": "^1.0.2",
    "@emotion/utils": "^1.0.0",
    "html-tag-interfaces": "^1.1.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
